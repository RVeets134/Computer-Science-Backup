
public class Car extends Ground{
	
	private String name;
	private int passengerLimit;
	
	public Car(String name,int passengerLimit) {
		
		super(name,passengerLimit);
		this.name = name;
		this.passengerLimit = passengerLimit;
		
	}
	
	public void move()
	{
		
		System.out.print(name + " is a car moving on four wheels.");
		
	}
	
	public String getName()
	{
		
		return name;
		
	}
	
	public int getPassengerLimit()
	{
		
		return passengerLimit;
		
	}
	
	public String toString()
	{
		
		return "Car" + "\n" + name + "\n" + "Maximum holding capacity: " + passengerLimit;
		
	}

}
