
public class dictionary extends book
{
	
	private int definitions;
	
	public dictionary (int numberOfPages, int numDefinitions)
	{
		super (numberOfPages);
		 definitions = numDefinitions;
	}
	public String toString()
	{

		String message = "Number of definitions: " + definitions + "\n" + "Definitions per page: "  + definitions/pages;
		return message;
	}
	public int getNumberOfDefinitions()
	{
		return definitions;
	}
}