
public class Store {

	public static void main(String[] args) {
		
		Vehicle joe = new Car("Joe",17,"SUV");
		Vehicle tom = new Sedan("Tom",420,"Sedan");
		Vehicle jack = new SUV("Jack",420,"SUV");
		Vehicle bill = new Motorcycle("Bill",420,"Motorcycle");
		Vehicle billy = new Airplane("Billy",420,"Airplane");
		Vehicle fred = new Jet("Fred",420,"Jet");
		Vehicle frederick = new Propeller("Frederick",420,"Propeller Plane");
		Vehicle john = new Helicopter("John",420,"Helicopter");
		Vehicle ben = new Boat("Ben",420,"Boat");
		Vehicle wonjae = new Submarine("Wonjae",420,"Submarine");
		
		Vehicle[] myVehicles = {joe,tom,jack,bill,billy,fred,frederick,john,ben,wonjae};
		
		System.out.println("Information for each vehicle and how they move.");
		System.out.println();
		
		for(int n = 0;n < myVehicles.length;n++)
		{
		
			System.out.println(myVehicles[n]);
			myVehicles[n].move();
			
		}
		
	}

}
