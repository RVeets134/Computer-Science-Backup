
public class Sedan extends Car{

	private String name;
	private int passengerLimit;
	
	public Sedan(String name,int passengerLimit) {
		
		super(name,passengerLimit);
		this.name = name;
		this.passengerLimit = passengerLimit;
		
	}
	
	public void move()
	{
		
		System.out.println(name + " is a sedan moving on four wheels.");
		
	}

	public String toString()
	{
		
		return "Type of Vehicle: Sedan" + "\n" + "Name of Vehicle " + name + "\n" + "Maximum holding capacity: " + passengerLimit;
		
	}

}
