
public class Car extends Ground{
	
	private String name;
	private int passengerLimit;
	
	public Car(String name,int passengerLimit) {
		
		super(name,passengerLimit);
		this.name = name;
		this.passengerLimit = passengerLimit;
		
	}
	
	public abstract void move()
	{
		
		System.out.print("The vehicle is moving on four wheels.")
		
	}

}
