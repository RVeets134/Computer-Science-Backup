
public class SUV extends Car{

	private String name;
	private int passengerLimit;
	
	public SUV(String name,int passengerLimit) {
		
		super(name,passengerLimit);
		this.name = name;
		this.passengerLimit = passengerLimit;
		
	}
	
	public void move()
	{
		
		System.out.println(name + " is a SUV moving on four wheels.");
		
	}

	public String toString()
	{
		
		return "Type of Vehicle: SUV" + "\n" + "Name of Vehicle " + name + "\n" + "Maximum holding capacity: " + passengerLimit;
		
	}

}