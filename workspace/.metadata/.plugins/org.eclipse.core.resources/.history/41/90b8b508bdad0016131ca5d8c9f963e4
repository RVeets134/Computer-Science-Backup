
public class novel extends book{
	
	private int characters;

	public novel(int numberOfPages,int numberOfCharacters) {

		super(numberOfPages);
		characters = numberOfCharacters;
		
	}
	public String toString()
	{

		String message = "Number of characters: " + characters;
		return message;
	}
	public int getNumberOfCharacters()
	{
		return characters;
	}

}
